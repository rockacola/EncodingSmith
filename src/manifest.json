{
    "name": "Encoding Smith",
    "description": "Chrome extension utility handles multiple encoding methods.",
    "version": "2.0.0",
    "icons": {
        "16": "images/favicon-16.png",
        "24": "images/favicon-24.png",
        "128": "images/favicon-128.png"
    },
    "app": {
        "launch": {
            "local_path": "index.html"
        }
    },
    "permissions": [
        "cookies",
        "unlimitedStorage"
    ],
    "manifest_version": 2
}